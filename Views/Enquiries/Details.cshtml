@model WONDERLUST_PROJECT_ADMINs.Models.Enquiry

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/Wonderlust.cshtml";
}

<div class="col-md-6 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Enquiry</h4>
            <p class="card-description">
                Details of Enquiry
            </p>
            <div class="forms-sample">
                <dl class="row">
                    <dt class="col-sm-6">
                       Enquiry ID
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.EnquiryId)
                    </dd>

                    <dt class="col-sm-6">
                        User ID
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.User.UserId)
                    </dd>
        <dt class = "col-sm-6">
           Contact
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ContactNumber)
        </dd>
        <dt class = "col-sm-6">
           Email
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EmailId)
        </dd>
        <dt class = "col-sm-6">
          Message
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Message)
        </dd>
        <dt class = "col-sm-6">
           IS Check
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IsBlock)
        </dd>
        <dt class = "col-sm-6">
Date Of Enquiry
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CreatedAt)
        </dd>
      
    </dl>
</div>
<div>
                <a asp-action="Edit" class="btn btn-info" asp-route-id="@Model.EnquiryId">Edit</a> |
                <a class="btn btn-warning" asp-action="Index">Back to List</a>
</div>
</div>
</div>
</div>
